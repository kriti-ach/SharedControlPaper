        <Param val="#***************************&amp;#10;&amp;#10;if goRTCount &gt; 0:&amp;#10;    goRTFeedback = goCumRT/goRTCount&amp;#10;    goRTFeedback = round(goRTFeedback, 2)&amp;#10;else:&amp;#10;    goRTFeedback = 'Null'&amp;#10;&amp;#10;if goTrialCount &gt; 0:&amp;#10;    commissionRate = (commissionCount/goTrialCount)&amp;#10;    commissionRate = round(commissionRate, 2)&amp;#10;    omissionRate = (omissionCount/goTrialCount)&amp;#10;    omissionRate = round(omissionRate, 2)&amp;#10;else: &amp;#10;    commissionRate = 'Null'&amp;#10;    omissionRate = 'Null'&amp;#10;&amp;#10;if stopTrialCount &gt; 0: &amp;#10;    probabilityOfStop = stopSuccessCount/stopTrialCount&amp;#10;    probabilityOfStop = round(probabilityOfStop, 2)&amp;#10;else:&amp;#10;    probabilityOfStop = 'Null'&amp;#10;&amp;#10;SSDFeedback = (SSD)/2&amp;#10;SSDFeedback = round(SSDFeedback, 2)&amp;#10;&amp;#10;testBlockCount += 1&amp;#10;block +=1&amp;#10;stopMessage = f&quot;You have completed {testBlockCount} of {numTestBlocks}.&quot;&amp;#10;&amp;#10;if testBlockCount == numTestBlocks:&amp;#10;    stopMessage += &quot;\n You have finished the testing phase.&quot;&amp;#10;else:&amp;#10;    stopMessage += &quot;\nPlease take this time to read your feedback if there is any, and take a 1 minute break.\n&quot;&amp;#10;    &amp;#10;    if commissionRate &gt; commision_thresh:&amp;#10;        stopMessage += &quot;\n Your accuracy is too low. Remember:&quot; \&amp;#10;                        + &quot;\n\tCircle: Index Finger&quot; \&amp;#10;                        + &quot;\n\tSquare: Middle Finger&quot;&amp;#10;                        &amp;#10;    if goRTFeedback != 'Null' and goRTFeedback &gt; rt_thresh:&amp;#10;        stopMessage += &quot;\nYou have been responding too slowly, please respond to each shape as quickly and as accurately as possible.&quot;&amp;#10;&amp;#10;    if omissionRate &gt; missed_response_thresh:&amp;#10;        if commissionRate &gt; commision_thresh:&amp;#10;            stopMessage += &quot;\n\nWe have detected a number of trials that required a response, where no response was made.  Please ensure that you are responding accurately and quickly to the shapes.&quot;&amp;#10;        else:&amp;#10;            stopMessage += &quot;\n\nWe have detected a number of trials that required a response, where no response was made.  Please ensure that you are responding accurately and quickly to the shapes.&quot; \&amp;#10;                        + &quot;\n Your accuracy is too low. Remember:&quot; \&amp;#10;                        + &quot;\n\tCircle: Index Finger&quot; \&amp;#10;                        + &quot;\n\tSquare: Middle Finger&quot;&amp;#10;&amp;#10;    if probabilityOfStop &lt; minStopCorrect:&amp;#10;        stopMessage += &quot;\nYou have not been stopping your response when stars are present.  Please try your best to stop your response if you see a star.&quot;&amp;#10;    elif probabilityOfStop &gt; maxStopCorrect:&amp;#10;        stopMessage += &quot;\nDo not slow down and wait for the star to appear. Please respond as quickly and accurately as possible when a star does not appear.&quot;&amp;#10;        &amp;#10;stopMessage += &quot;\n\nPress enter to continue.&quot;&amp;#10;&amp;#10;&amp;#10;print(f'comission_rate: {commissionRate}, omission_rate: {omissionRate}')&amp;#10;print(stopMessage)&amp;#10;&amp;#10;#***************************&amp;#10;&amp;#10;stopTrialList = createTrialTypes(numTrialsPerBlock, color, shapes, conditions, totalShapesUsed)&amp;#10;&amp;#10;thisExp.addData(f&quot;Test Block: {testBlockCount}&quot;, stopTrialList)" valType="extendedCode" updates="constant" name="Begin Routine"/>
