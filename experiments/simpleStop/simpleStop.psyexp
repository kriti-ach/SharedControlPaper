      <CodeComponent name="test_block_feedback" plugin="None">
        <Param val="&amp;#10;maxSSD = 1&amp;#10;minSSD = 0&amp;#10;&amp;#10;rt_thresh = 1000&amp;#10;missed_response_thresh = 0.1&amp;#10;commision_thresh = 0.25&amp;#10;&amp;#10;maxStopCorrect = 0.7&amp;#10;minStopCorrect = 0.3" valType="extendedCode" updates="constant" name="Before Experiment"/>
